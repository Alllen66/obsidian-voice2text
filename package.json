{
  "name": "obsidian-voice2text",
  "version": "1.0.0",
  "description": "将语音转换为文字并保存到Obsidian笔记中，支持中文识别",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "voice",
    "text",
    "whisper",
    "openai",
    "chinese"
  ],
  "author": "xuefeng.ai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/allenliu123/obsidian-voice2text.git"
  },
  "dependencies": {
    "obsidian": "latest"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.17.3",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0"
  }
}
